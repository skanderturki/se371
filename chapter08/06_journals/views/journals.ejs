<!DOCTYPE html>
<html lang="en">
<head>
  <!-- general meta tags -->
  <meta charset="utf-8">
  <meta name="author" content="">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <!-- site specific tags -->
  <title>Journals</title>
  <!-- style sheets -->
  <link rel="stylesheet" href="/css/styles.css">

</head>
<body>
  <header>
    <h1>Journals (<%= journals.length %> results)</h1>
  </header>
  <main>
    <section>
      <article>
        <table id="journals">
          <thead>
            <tr><th>Rank</th><th>Title</th><th>Type</th><th>SJR</th><th>SJR_Best_Quartile</th>
              <th>H_index</th><th>Country</th><th>Publisher</th><th>Categories</th></tr>
          </thead>
          <tbody>
            <% journals.forEach( (journal) => { %>
              <tr><td><%= journal.Rank %></td><td><%= journal.Title %></td>
                <td><%= journal.Type %></td><td><%= journal.SJR %></td><td><%= journal.SJR_Best_Quartile %></td>
                <td><%= journal.H_index %></td><td><%= journal.Country %></td><td><%= journal.Publisher %></td><td><%= journal.Categories %></td>
              </tr>
            <% }); %>
          </tbody>
        </table>
      </article>
    </section>
  </main>
  <footer>
    <p>@Copyright 2022</p>
  </footer>
</body>
</html>